{
  "name": "electron-test",
  "private": true,
  "version": "1.0.0",
  "main": "dist/electron/index.js",
  "scripts": {
    "dev": " concurrently \"vite\" \"node ../NeteaseCloudMusicApi/app.js\"",
    "start": "vite",
    "build": "vite build",
    "build-electron": "rimraf dist && vite build && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "@lrc-player/core": "^1.1.4",
    "@lrc-player/parse": "^1.0.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@types/cookie": "^0.5.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "axios": "^0.27.2",
    "colorthief": "^2.4.0",
    "compressorjs": "^1.2.1",
    "electron-log": "^5.1.2",
    "gsap": "^3.12.5",
    "js-cookie": "^3.0.1",
    "less": "^4.1.3",
    "pinia": "^2.0.14",
    "postcss": "^8.4.49",
    "province-city-china": "^8.5.6",
    "qrcode": "^1.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "tailwindcss": "^3.4.15",
    "three": "^0.166.1",
    "vite": "^5.4.11",
    "vue": "^3.2.25",
    "vue-router": "^4.0.16",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/gsap": "^3.0.0",
    "@types/js-cookie": "^3.0.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "concurrently": "^8.2.2",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "electron-devtools-installer": "^3.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.9.1",
    "vite-plugin-electron": "^0.5.1",
    "vue-tsc": "^0.34.7"
  },
  "build": {
    "appId": "com.electron.desktop",
    "productName": "Music",
    "asar": true,
    "copyright": "Copyright Â© 2024 YSH",
    "directories": {
      "output": "release/${productName}"
    },
    "files": [
      "dist"
    ],
    "mac": {
      "icon": "./src/assets/logo.ico",
      "artifactName": "${productName}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "./src/assets/logo.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  }
}
